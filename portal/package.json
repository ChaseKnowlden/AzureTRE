{
  "name": "portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "buildConfig:nix": "export TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"}",
    "buildConfig:win32": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}",
    "buildConfig": "run-script-os",
    "start": "npm run buildConfig && ng serve",
    "start-mock": "dotnet build ../PortalApi/Dre.Core.Api/ && prism mock --port 5000 ../PortalApi/Dre.Core.Api/swagger.json",
    "start-with-mock": "concurrently \"npm run start-mock\" \"npm run start\"",
    "build": "npm run buildConfig && ng build",
    "prod": "npm run buildConfig && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.1.3",
    "@angular/localize": "^12.1.3",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@azure/msal-angular": "^2.0.2",
    "@azure/msal-browser": "^2.16.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@jsonforms/angular": "^3.0.0-alpha.0",
    "@jsonforms/angular-material": "^3.0.0-alpha.0",
    "@jsonforms/core": "^3.0.0-alpha.0",
    "angular-auth-oidc-client": "^11.5.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.8.3",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "material-design-icons": "^3.0.1",
    "ngx-spinner": "^10.0.1",
    "office-ui-fabric-core": "^11.0.0",
    "popper.js": "1.16.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
